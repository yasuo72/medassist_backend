# Rename this file to .env and fill in the values before running locally.

# HuggingFace Space API URL for medical report summarizer
# Example: https://my-summarizer-space.hf.space/run/predict
HF_SPACE_API_URL=
# Optional: Set if your Space is private / gated
HF_API_TOKEN=

# MongoDB connection string
MONGO_URI=

# JWT secret for signing auth tokens
JWT_SECRET=

# Google OAuth2 Credentials (Optional)
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=

# Twilio (Optional - for SMS alerts)
TWILIO_ACCOUNT_SID=
TWILIO_AUTH_TOKEN=
TWILIO_PHONE_NUMBER=

# Session secret for express-session (used in Google OAuth flow)
SESSION_SECRET=

# Any other variables you add in the future should be documented here.
